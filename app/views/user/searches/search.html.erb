<table class="table table-hover table-inverse">
  <!--検索対象モデルがUserの時 -->
  <% if @range == "User" %>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.username %></td>
        </tr>
      <% end %>
    </tbody>
  <% else %>
    <!--検索対象モデルがUserではない時(= 検索対象モデルがTweetの時) -->
    <tbody>
      <% @tweets.each do |tweet| %>
        <tr>
          <td><%= tweet.content %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
</table>